{
  "name": "example",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-scripts": "^2.11.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-example",
    "stop": "egg-scripts stop --title=egg-server-example",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "start_test": "pm2 start test.ecosystem.config.js --env test",
    "start_preview": "pm2 start preview.ecosystem.config.js --env pre",
    "start_production": "pm2 start production.ecosystem.config.js --env production",
    "restart_test": "pm2 restart test.ecosystem.config.js --env test",
    "restart_preview": "pm2 restart preview.ecosystem.config.js --env pre",
    "restart_production": "pm2 restart production.ecosystem.config.js --env production"
  },
  "ci": {
    "version": "10"
  },
